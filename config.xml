<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="52" id="com.hiddenfriends.apprelease" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hidden Friends</name>
    <description>
    </description>
    <author email="tomaz.jr@gmail.com" href="">
        tomaz junior
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="/www/icons/icons-192.png" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Fullscreen" value="true" />
    <preference name="orientation" value="landscape" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
</widget>
